{
  "id": "max/plantarium/branches",
  "outputs": [
    "path"
  ],
  "inputs": {
    "plant": {
      "type": "path",
      "external": true
    },
    "length": {
      "type": "float",
      "min": 0,
      "max": 3,
      "step": 0.05,
      "value": 0.8
    },
    "thickness": {
      "type": "float",
      "min": 0,
      "max": 1,
      "step": 0.01,
      "value": 0.8
    },
    "offsetSingle": {
      "type": "float",
      "min": 0,
      "hidden": true,
      "max": 1,
      "step": 0.01,
      "value": 0.5
    },
    "lowestBranch": {
      "type": "float",
      "hidden": true,
      "min": 0,
      "max": 1,
      "step": 0.01,
      "value": 0.2
    },
    "highestBranch": {
      "type": "float",
      "hidden": true,
      "min": 0,
      "max": 1,
      "step": 0.01,
      "value": 1
